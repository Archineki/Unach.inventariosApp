<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Unach.inventariosApp.Vistas.PrincipalPage"
             xmlns:ViewModel="clr-namespace:Unach.InventarioApp.Core.ViewModel;assembly=Unach.InventarioApp.Core">
    <ContentPage.BindingContext>
        <ViewModel:PrincipalPageViewModel/>
    </ContentPage.BindingContext>
<ContentPage.Content>
        <StackLayout>
            <Label Text="Bienvenido a la Pagina Principal"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Regresar a Login" Command="{Binding RegresarCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>